    <script src="~/Scripts/jquery.ui.widget.js"></script>
    <script src="~/Scripts/jquery.uploadfile.min.js"></script>

    @RenderSection("scripts", required: false)
    <style>

        .col-form-label {
            font-weight: 600;
        }
    </style>
    <script>
        $(document).ready(function () {
        showHideSubMenus();
        selectMenu();
        //LoadNotification();
        var notifyCnt= @HttpContext.Current.Session["NotifyCount"].ToString()
        $('#spBadgeCnt').html(notifyCnt);
    });

    function LoadNotification() {
            //alert(123);
        $.ajax(
            {
                type: 'GET',
                data: "",
                url: $("#btnloadNotification").data('url'),
                success: function (result) {
                    console.log("alertlst");
                        console.log(result);
                        $('#dvNotification').html(result);
                       //('#spBadgeCnt').html(result.msgCount);
                    //if (parseInt(json.msgCount) == 0) {
                    //    $('#ulNotifyMenu').hide();
                    //}
                },
                error: function (error) {
                    console.log("error");
                    console.log(error);
                }
            });
    }

    function showHideSubMenus() {
     var userName = ""; var menuList = "";
     userName = '@HttpContext.Current.Session["UserName"].ToString()';
        menuList = '@HttpContext.Current.Session["pageAccessList"].ToString()';
        console.log(menuList);
        if (menuList != '') {
            var array = menuList.split(",");
            for (i = 0; i < array.length; i++) {
                $("#" + array[i].trim() + "").show();
            }
        }
    }


    function selectMenu() {
        var cntrlName = window.location.pathname;
        console.log(cntrlName);
        $(".nav-item").removeClass('active');

        if (cntrlName == "/Dashboard" || cntrlName == "/Login/Dashboard" || cntrlName == "/Dashboard/Index" ) {
            $("#liDashboard").addClass("active");
        }
        else if (cntrlName == "/Nomination" || cntrlName == "/Login/Nomination" || cntrlName == "/Nomination/Index") {
            $("#liNominationlst").addClass("active");
        }
        else if (cntrlName == "/Evaluation" || cntrlName == "/Login/Evaluation" || cntrlName == "/Evaluation/Index") {
            $("#liEvaluationlst").addClass("active");
        }
        else if (cntrlName == "/Nomination/Nomination" || cntrlName == "/Login/Nomination/Nomination" ) {
            $("#liAddNomination").addClass("active");
        }
        else if (cntrlName == "/PanelMembers" || cntrlName == "/Login/PanelMembers" || cntrlName == "/PanelMembers/Index") {
            $("#liPanelMembers").addClass("active");
        }
        else if (cntrlName == "/TQCHead" || cntrlName == "/Login/TQCHead" || cntrlName == "/TQCHead/Index") {
            $("#liTQCHead").addClass("active");
        }
        else if (cntrlName == "/Configuration" || cntrlName == "/Login/Configuration" || cntrlName == "/Configuration/Index") {
            $("#liSettings").addClass("active");
        }
        else if (cntrlName == "/StarOfMonth" || cntrlName == "/StarOfMonth/Index") {
            $("#liStarOfMonth").addClass("active");
        }

        else if (cntrlName == "/Login/Nomination/PendingNomination" || cntrlName == "/Nomination/PendingNomination") {
            $("#liNominationPendinglst").addClass("active");
        }

        else if (cntrlName == "/Login/Nomination/ApproveNomination" || cntrlName == "/Nomination/ApproveNomination") {
            $("#liNominationApprovallst").addClass("active");
        }
        else if (cntrlName == "/Evaluation/Evaluationlst") {
            $("#liViewEvaluationlst").addClass("active");
        }


    }
    </script>
</body>

</html>
